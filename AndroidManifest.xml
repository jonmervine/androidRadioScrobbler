<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.darkmage530.androidRadioScrobbler"
          android:versionCode="1"
          android:versionName="1.0">
    <uses-sdk android:minSdkVersion="17"/>
    <application android:label="@string/app_name" android:icon="@drawable/ic_launcher">
        <activity android:name="MyActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name="com.darkmage530.androidRadioScrobbler.DisplayMessageActivity"
                android:label="@string/title_activity_display_message"
                android:parentActivityName="com.darkmage530.androidRadioScrobbler.MyActivity" >
            <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value="com.darkmage530.androidRadioScrobbler.MyActivity" />
        </activity>
        <activity
            android:name="com.darkmage530.androidRadioScrobbler.MusicPlayer"
            android:label="@string/title_activity_music_player"
            android:parentActivityName="com.darkmage530.androidRadioScrobber.MyActivity">
                <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value="com.darkmage530.androidRadioScrobbler.MyActivity"/>
        </activity>
    </application>
</manifest>
